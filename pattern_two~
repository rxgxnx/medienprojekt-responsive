 <!-- Java Script --> 
  
  <script src="js/vendor/jquery.js"></script>
  <script src="js/foundation/foundation.js"></script>
  <script src="js/foundation/foundation.offcanvas.js"></script>
   <script src="js/foundation/foundation.search.js"></script>
  
    <script>
      $(document).foundation();
    </script>
    
    <script>


$(document).ready(function(){

//ACCORDION BUTTON ACTION
$('div.accordionButton_vendor').click(function() {
//IF THE DIV YOU CLICKED IS ALREADY OPEN, CLOSE AND REMOVE THE OPEN CLASS
if ($(this).next().hasClass('openDiv')) {
$('div.accordionContent_vendor.openDiv').slideUp('normal');
$('div.accordionContent_vendor.openDiv').removeClass('openDiv');
}
//CLOSE ANY OPEN DIVS, OPEN THE CLICKED DIV
else {
$('div.accordionContent_vendor.openDiv').slideUp('normal');
$('div.accordionContent_vendor.openDiv').removeClass('openDiv');
$(this).next().slideDown('normal');
$(this).next().addClass('openDiv');
}
});
//HIDE THE DIVS ON PAGE LOAD
$("div.accordionContent_vendor").hide();

});

</script>
    
 
